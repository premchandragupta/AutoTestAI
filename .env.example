
# Postgres (Supabase)
POSTGRES_USER=autotestai
POSTGRES_PASSWORD=autotestai_pass
POSTGRES_DB=autotestai_db
POSTGRES_PORT=5432
# or set full DATABASE_URL for Render/Supabase
DATABASE_URL=postgresql+asyncpg://autotestai:autotestai_pass@db.example.supabase.co:5432/autotestai_db?sslmode=require

# OpenAI
OPENAI_API_KEY=sk-REPLACE

# Redis (managed production)
# Example: rediss://:PASSWORD@host:port/0
REDIS_URL=redis://redis:6379/0

# AI cache
AI_CACHE_TTL=3600
AI_CACHE_MAX=1024

# Rate limiting
RATE_LIMIT=60
RATE_PERIOD=60

# JWT
JWT_SECRET=change_this_secret_in_prod
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS
ALLOWED_ORIGINS=http://localhost:3000

# Frontend
VITE_BACKEND_URL=http://localhost:8000
